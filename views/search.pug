extends layouts/main
include mixins/videoBlock


block content
    .search__header
        h3 Searching for: #{searchingBy}
    .search__videos
        if video.length ==0
            h5 No Videos Found
        each item in video
            +videoBlock({
                id:item.id,
                title:item.title,
                views:item.views,
                videoFile:item.videoFile
            })